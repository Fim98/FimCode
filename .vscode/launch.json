{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "bun",
      "request": "launch",
      "name": "[Bun] 调试 index.ts",
      "program": "${workspaceFolder}/index.ts",
      "cwd": "${workspaceFolder}",
      "args": [],
      "noDebug": false
    },
    {
      "type": "bun",
      "request": "launch",
      "name": "[Bun] 调试当前文件",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "noDebug": false
    },
    {
      "type": "bun",
      "request": "launch",
      "name": "[Bun] 运行当前文件",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "noDebug": true
    },
    {
      "type": "bun",
      "request": "attach",
      "name": "[Bun] 附加到进程",
      "url": "ws://localhost:6499/",
      "stopOnEntry": false
    }
  ]
}
